<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>
    <AssemblyName>StackExchange.Exceptional.MySQL</AssemblyName>
    <OutputType>Library</OutputType>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <Authors>Nick Craver</Authors>
    <Description>MySQL storage provider for StackExchange.Exceptional</Description>
    <PackageId>StackExchange.Exceptional.MySQL</PackageId>
    <PackageVersion>1.0.0.0</PackageVersion>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/NickCraver/StackExchange.Exceptional</PackageProjectUrl>
    <PackageTags>SQL MySQL Exception Handler Errors Stack Exchange Exceptional</PackageTags>
    <IncludeSymbols>true</IncludeSymbols>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\StackExchange.Exceptional\StackExchange.Exceptional.csproj" />
    <PackageReference Include="Dapper" Version="1.42" />
    <PackageReference Include="MySql.Data" Version="6.8.3" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>
</Project>